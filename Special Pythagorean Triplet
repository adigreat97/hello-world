double i1,i2,i3;                                     
for(int a=1;a<=1000;a++)
{
    for(int b=1;b<=a;b++)
{
   for(int c=1;c<=b;c++)
{
    i1=Math.pow(a,2);
    i2=Math.pow(b,2);
    i3=Math.pow(c,2);
    if(i1==(i2+i3) && (a+b+c)==1000)
    {
     System.out.println(a*b*c);
    }
        
}         
